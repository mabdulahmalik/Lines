{
  "name": "linesapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "codegen": "npm run schema-gen && npm run code-gen",
    "lint": "eslint",
    "serve": "cross-env PORT=443 serve dist",
    "code-gen": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 graphql-codegen --config codegen.ts",
    "schema-gen": "dotnet run --project ../Platform/src/SOL.Gateway -- schema export --output ../../../LinesApp/schema.graphql"
  },
  "dependencies": {
    "@apollo/client": "^3.11.4",
    "@vue/apollo-composable": "^4.2.0",
    "@vue/apollo-util": "^4.0.0-beta.6",
    "@vueuse/core": "^10.11.0",
    "apollo-upload-client": "^18.0.1",
    "cropperjs": "^1.6.2",
    "dayjs": "^1.11.12",
    "flowbite": "^2.4.1",
    "flowbite-vue": "^0.1.5",
    "graphql": "^16.9.0",
    "graphql-tag": "^2.12.6",
    "moment": "^2.30.1",
    "pinia": "^2.1.7",
    "subscriptions-transport-ws": "^0.11.0",
    "vee-validate": "^4.13.2",
    "vue": "^3.4.29",
    "vue-cropperjs": "^5.0.0",
    "vue-multiselect": "^3.0.0",
    "vue-router": "^4.4.0",
    "vue-tailwind-datepicker": "^1.7.3",
    "vue-toastification": "^2.0.0-rc.5",
    "vuedraggable": "^4.1.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/client-preset": "4.3.3",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-codegen/typescript": "^4.0.9",
    "@graphql-codegen/typescript-operations": "^4.2.3",
    "@graphql-codegen/typescript-vue-apollo": "^4.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@types/apollo-upload-client": "^18.0.0",
    "@types/jquery": "^3.5.32",
    "@types/node": "^20.14.10",
    "@typescript-eslint/parser": "^8.24.1",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.15.0",
    "postcss": "^8.4.39",
    "prettier": "^3.5.1",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^5.4.14",
    "vite-plugin-pwa": "^0.21.1",
    "vue-tsc": "^2.0.21"
  }
